# AI Student Assistant Environment Configuration
# Copy this file to .env and fill in your actual values

# Hugging Face API Configuration
# Get your API token from: https://huggingface.co/settings/tokens
HF_API_KEY=

# AI Model Configuration (optional - defaults provided)
HF_MODEL_CHAT=microsoft/DialoGPT-medium
HF_MODEL_SUMMARY=facebook/bart-large-cnn
HF_MODEL_EMBEDDINGS=sentence-transformers/all-MiniLM-L6-v2

# Rate Limiting
AI_DAILY_QUOTA=20

# Backend Configuration
PROXY_BASE_URL=http://localhost:3000

# Firebase Configuration (for Flutter app)
# These will be set automatically by flutterfire configure
# But you can override them here if needed
# FIREBASE_PROJECT_ID=your-project-id
# FIREBASE_API_KEY=your-api-key