{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "AI Student Assistant (Web, OpenRouter)",
      "type": "dart",
      "request": "launch",
      "flutterMode": "debug",
      "args": [
        "-d",
        "chrome",
  // dotenv-based key loading; no dart-define required
      ]
    },
    {
      "name": "AI Student Assistant (Android, OpenRouter)",
      "type": "dart",
      "request": "launch",
      "flutterMode": "debug",
      "args": [
        "-d",
        "android",
  // dotenv-based key loading; no dart-define required
      ]
    },
    {
      "name": "Flutter (Web with OpenRouter defines)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "deviceId": "chrome",
      "toolArgs": [
        "--dart-define=OPENROUTER_API_KEY=${env:OPENROUTER_API_KEY}",
        "--dart-define=OPENROUTER_MODEL=deepseek/deepseek-chat-v3.1:free"
      ]
    },
    {
      "name": "Flutter (Android device)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart"
    },
    {
      "name": "Flutter (Web, use URL param)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "deviceId": "chrome"
    }
  ]
}
